<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="KitBox.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:KitBox"
    xmlns:views="clr-namespace:KitBox.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="KitBox">
    <TabBar> 
        
        <Tab Title="ChoicePage">
            <ShellContent
                Title="ChoicePage"
                ContentTemplate="{DataTemplate views:ChoicebtwPage}"/>
        </Tab>
        
       
        
        
        <Tab Title="Panier">
            <ShellContent
                Title="Panier"
                ContentTemplate="{DataTemplate views:Panier}"/>
            
        </Tab>
       
    </TabBar>
</Shell>
